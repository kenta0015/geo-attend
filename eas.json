{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": { "buildType": "apk" },
      "env": { "APP_ENV": "internal" },
      "channel": "internal",
      "developmentClient": false,
      "distribution": "internal"
    },
    "internal": {
      "android": { "buildType": "app-bundle" },
      "env": { "APP_ENV": "internal" },
      "channel": "internal",
      "developmentClient": false,
      "distribution": "internal",
      "autoIncrement": true
    },
    "production": {
      "android": { "buildType": "app-bundle" },
      "env": { "APP_ENV": "production" },
      "channel": "production",
      "developmentClient": false,
      "autoIncrement": true
    },
    "development-simulator": {
      "env": { "APP_ENV": "internal" },
      "channel": "internal",
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "simulator": true }
    },
    "development": {
      "env": { "APP_ENV": "internal" },
      "channel": "internal",
      "developmentClient": true,
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      },
      "ios": {
        "ascAppId": "6758450393"
      }
    }
  }
}
